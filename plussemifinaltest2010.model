if rnd() <0.5 then my.X = 42
else my.X = 48 end if
my.Y = rnd() * 100 my.home = 0 my.people =0 my.strength = 5000 my.younglabor = 0 my.youngcon = 0 my.middlelabor = 0 my.middlecon = 0 my.oldlabor = 0 my.oldcon = 0 my.t_people = 0 my.totalvalue = 0 }
Agt_Step{ my.t_people = my.t_people + my.people
if getcountstep() Mod 250 == 0 then
	my.t_people = 0
end if
my.value = 0
include"valuetest.inc"
universe.sumpeople = universe.sumpeople + my.people
my.strength = my.strength - 1 - (my.people - gethistory(my.people, 1))
universe.bridgestrength = universe.bridgestrength + my.strength
}
