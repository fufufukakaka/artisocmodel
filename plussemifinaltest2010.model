//-------------------------------------------------------
// Agent Rule
//-------------------------------------------------------
#begin_rule UNIVERSE
Univ_Init{
universe.sumpeople = 0
universe.younglabor = 0
universe.middlelabor = 0
universe.oldlabor = 0
universe.youngcon = 0
universe.middlecon = 0
universe.oldcon = 0
dim i as integer
dim bridgenumber as agtset
for i = 0 to universe.bridgenum - 1
	createagt(universe.triage.bridge)
next i

}

Univ_Step_Begin{
universe.totalpeople = countagt(universe.triage.young) + countagt(universe.triage.middle) + countagt(universe.triage.old)
if getcountstep() > 5 then
universe.averagevalue = universe.totalvalue / universe.numberofbridge 
end if
universe.hapsum = 0 universe.bridgestrength = 0 universe.salary = 0 universe.youngsalary = 0 universe.middlesalary = 0 universe.oldsalary = 0 universe.utilize = 0
universe.totalvalue = 0
universe.youngutilize = 0 universe.middleutilize = 0 universe.oldutilize = 0 universe.t_utilize = 0 universe.youngdist = 0 universe.middledist = 0 universe.olddist = 0
}

Univ_Step_End{
if getcountstep() Mod 250 == 2 then
	universe.youngutilize = 0
	universe.middleutilize = 0
	universe.oldutilize = 0
	universe.t_utilize = 0
end if
openfilecsv("output.txt", 2, 3)
writefilecsv(2, getcountstep(), false)
writefilecsv(2, universe.salary, false)
writefilecsv(2, universe.youngsalary, false)
writefilecsv(2, universe.middlesalary, false)
writefilecsv(2, universe.oldsalary, false)
writefilecsv(2, getcountsimulationnumber(), true)
closefilecsv(2)
}

Univ_Finish{
println(getcountsimulationnumber())
}
#end_rule UNIVERSE




